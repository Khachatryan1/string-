#include <iostream>
#include <string>
using namespace std;

int main()
{
    string text = "Python is an interpreted high-level general-purpose programming language. Its design philosophy emphasizes code readability with its use of significant indentation. Its language constructs as well as its object-oriented approach aim to help programmers write clear, logical code for small and large-scale projects";
    string word ;
    cin >> word;
    size_t idx1 = text.find(word);
    size_t idx2 = idx1 + word.size() - 1;
    cout << idx1 << " " << idx2 << "\n";

    return 0;
}
